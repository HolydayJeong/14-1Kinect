<Window
        x:Class="SungJik_SungHwa.Dibi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:gif="http://wpfanimatedgif.codeplex.com"
        Title="Dibi" Height="960" Width="1280" Loaded="Window_Loaded" Closing="Window_Closing">
    <Grid>
        <Canvas HorizontalAlignment="Left" Height="920" Margin="0,10,-8,0" VerticalAlignment="Top" Width="1280" Grid.ColumnSpan="3">
            <Image HorizontalAlignment="Left" x:Name="kinect1" Height="914" Width="1280" Stretch="Fill"  VerticalAlignment="Top" Canvas.Top="20"/>
            <Image HorizontalAlignment="Left" x:Name="Soong" Height="158" VerticalAlignment="Top" Width="136" RenderTransformOrigin="0.5,0.5"  Canvas.Left="592" Canvas.Top="356"/>
            <Image Height="600" Name="monkey" Canvas.Left="700" Canvas.Top="284" Width="490" RenderTransformOrigin="0.5,0.5"/>
            <Image Name="skip" Height="87" Width="143" Canvas.Left="801" Canvas.Top="198"/>
            <Image Name="db1" Height="219" Canvas.Left="142" Canvas.Top="108" Width="238" RenderTransformOrigin="0.5,0.5">
                <Image.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform Angle="-32.09"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Image.RenderTransform>
            </Image>
            <Image Name="db2" Height="206" Canvas.Left="553" Canvas.Top="33" Width="217"/>
            <Image Name="dip" Height="237" Canvas.Left="969" Canvas.Top="87" Width="246" RenderTransformOrigin="0.812,0.491">
                <Image.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform Angle="23.846"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Image.RenderTransform>
            </Image>
            <Image Name="Alertimg" Height="210" Width="436" Canvas.Left="34" Canvas.Top="518" />
            <TextBlock Name="score" Foreground="Green" FontSize="100" Canvas.Left="34" TextWrapping="Wrap" Text="0 : 0" Canvas.Top="33" Height="134" Width="206"  Visibility="Hidden"/>
            <Image Name="notice" Canvas.Left="170" Canvas.Top="10" />
            <Image Name="replay" Height="124" Canvas.Left="440" Canvas.Top="310" Width="124"/>
            <Image Name="home" Height="124" Canvas.Left="728" Canvas.Top="310" Width="124"/>
        </Canvas>
        <!--        <MediaElement Name="video" HorizontalAlignment="Left" Height="287" VerticalAlignment="Top"  Width="507"  Grid.Column="2" Margin="217,563,0,0" LoadedBehavior="Manual" /> -->

    </Grid>
</Window>
